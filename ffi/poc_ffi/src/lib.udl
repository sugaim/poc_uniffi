namespace poclib {
  enum OptionType {
    "Call",
    "Put",
  };

  [Traits=(Debug, Display)]
  interface Error {    
  };

  dictionary BsModel {
    f64 spot;
    f64 vol;
  };

  [Throws=Error]
  f64 bs_call(f64 spot, f64 strike, f64 vol, f64 rate, f64 time);

  [Throws=Error]
  f64 bs_prem(BsModel model, f64 strike, f64 rate, f64 time, OptionType option_type);
}
